<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_ClientCreation" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.42.0">
  <bpmn:process id="ClientCreationProcess" name="Client Creation Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_ClientCreation" name="Start">
      <bpmn:outgoing>Flow_SearchClient</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:serviceTask id="Task_SearchClient" name="Search Client by Email">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="search-client" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_SearchClient</bpmn:incoming>
      <bpmn:outgoing>Flow_ClientDecision</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_ClientExists" name="Client Exists?">
      <bpmn:incoming>Flow_ClientDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_UseExisting</bpmn:outgoing>
      <bpmn:outgoing>Flow_CreateNew</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_CreateClient" name="Create New Client">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="create-client" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_CreateNew</bpmn:incoming>
      <bpmn:outgoing>Flow_Merge</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_ClientReady" name="Client Ready">
      <bpmn:incoming>Flow_UseExisting</bpmn:incoming>
      <bpmn:incoming>Flow_Merge</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_SearchClient" sourceRef="StartEvent_ClientCreation" targetRef="Task_SearchClient" />
    <bpmn:sequenceFlow id="Flow_ClientDecision" sourceRef="Task_SearchClient" targetRef="Gateway_ClientExists" />
    <bpmn:sequenceFlow id="Flow_CreateNew" name="Not Found" sourceRef="Gateway_ClientExists" targetRef="Task_CreateClient">
      <bpmn:conditionExpression xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="bpmn:tFormalExpression">=clientFound = false</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_UseExisting" name="Found" sourceRef="Gateway_ClientExists" targetRef="EndEvent_ClientReady">
      <bpmn:conditionExpression xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="bpmn:tFormalExpression">=clientFound = true</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Merge" sourceRef="Task_CreateClient" targetRef="EndEvent_ClientReady" />
  </bpmn:process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_ClientCreation">
    <bpmndi:BPMNPlane id="BPMNPlane_ClientCreation" bpmnElement="ClientCreationProcess">
      <bpmndi:BPMNShape id="_BPMNShape_StartEvent_ClientCreation" bpmnElement="StartEvent_ClientCreation">
        <dc:Bounds x="179" y="169" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="185" y="212" width="25" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_SearchClient_di" bpmnElement="Task_SearchClient">
        <dc:Bounds x="270" y="147" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_ClientExists_di" bpmnElement="Gateway_ClientExists" isMarkerVisible="true">
        <dc:Bounds x="432" y="162" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="476" y="180" width="67" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_CreateClient_di" bpmnElement="Task_CreateClient">
        <dc:Bounds x="530" y="230" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_ClientReady_di" bpmnElement="EndEvent_ClientReady">
        <dc:Bounds x="672" y="102" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="658" y="78" width="64" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow_SearchClient_di" bpmnElement="Flow_SearchClient">
        <di:waypoint x="215" y="187" />
        <di:waypoint x="270" y="187" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_ClientDecision_di" bpmnElement="Flow_ClientDecision">
        <di:waypoint x="370" y="187" />
        <di:waypoint x="432" y="187" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_CreateNew_di" bpmnElement="Flow_CreateNew">
        <di:waypoint x="452" y="207" />
        <di:waypoint x="452" y="270" />
        <di:waypoint x="530" y="270" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="464" y="243" width="52" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_UseExisting_di" bpmnElement="Flow_UseExisting">
        <di:waypoint x="452" y="167" />
        <di:waypoint x="452" y="120" />
        <di:waypoint x="670" y="120" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="594" y="130" width="32" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_Merge_di" bpmnElement="Flow_Merge">
        <di:waypoint x="630" y="270" />
        <di:waypoint x="690" y="270" />
        <di:waypoint x="690" y="138" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>

