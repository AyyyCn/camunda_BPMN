<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.42.0">
  <bpmn:process id="HotelReservationProcess" name="Hotel Reservation Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_Reservation" name="Reservation Request">
      <bpmn:outgoing>Flow_Validate</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:serviceTask id="Task_ValidateInput" name="Validate Input">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="validate-input" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Validate</bpmn:incoming>
      <bpmn:outgoing>Flow_CheckClient</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:callActivity id="SubProcess_CreateClient" name="Create or Find Client" calledElement="ClientCreationProcess">
      <bpmn:incoming>Flow_CheckClient</bpmn:incoming>
      <bpmn:outgoing>Flow_CheckRooms</bpmn:outgoing>
    </bpmn:callActivity>
    <bpmn:serviceTask id="Task_CheckRoomAvailability" name="Check Room Availability">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="check-room-availability" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_CheckRooms</bpmn:incoming>
      <bpmn:outgoing>Flow_RoomDecision</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_RoomAvailable" name="Room Available?">
      <bpmn:incoming>Flow_RoomDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_BlockRoom</bpmn:outgoing>
      <bpmn:outgoing>Flow_NoRoom</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_BlockRoom" name="Block Room">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="block-room" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_BlockRoom</bpmn:incoming>
      <bpmn:outgoing>Flow_CreateBooking</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_CreateBooking" name="Create Booking">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="create-booking" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_CreateBooking</bpmn:incoming>
      <bpmn:outgoing>Flow_Success</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_Success" name="Reservation Created">
      <bpmn:incoming>Flow_Success</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="EndEvent_NoRoom" name="No Room Available">
      <bpmn:incoming>Flow_NoRoom</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:boundaryEvent id="BoundaryEvent_Error" name="Error" attachedToRef="Task_ValidateInput">
      <bpmn:outgoing>Flow_Error</bpmn:outgoing>
      <bpmn:errorEventDefinition id="ErrorEventDefinition_1" />
    </bpmn:boundaryEvent>
    <bpmn:endEvent id="EndEvent_Error" name="Error">
      <bpmn:incoming>Flow_Error</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_Validate" sourceRef="StartEvent_Reservation" targetRef="Task_ValidateInput" />
    <bpmn:sequenceFlow id="Flow_CheckClient" sourceRef="Task_ValidateInput" targetRef="SubProcess_CreateClient" />
    <bpmn:sequenceFlow id="Flow_CheckRooms" sourceRef="SubProcess_CreateClient" targetRef="Task_CheckRoomAvailability" />
    <bpmn:sequenceFlow id="Flow_RoomDecision" sourceRef="Task_CheckRoomAvailability" targetRef="Gateway_RoomAvailable" />
    <bpmn:sequenceFlow id="Flow_BlockRoom" name="Yes" sourceRef="Gateway_RoomAvailable" targetRef="Task_BlockRoom">
      <bpmn:conditionExpression xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="bpmn:tFormalExpression">=${roomAvailable == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_NoRoom" name="No" sourceRef="Gateway_RoomAvailable" targetRef="EndEvent_NoRoom">
      <bpmn:conditionExpression xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="bpmn:tFormalExpression">=${roomAvailable == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_CreateBooking" sourceRef="Task_BlockRoom" targetRef="Task_CreateBooking" />
    <bpmn:sequenceFlow id="Flow_Success" sourceRef="Task_CreateBooking" targetRef="EndEvent_Success" />
    <bpmn:sequenceFlow id="Flow_Error" sourceRef="BoundaryEvent_Error" targetRef="EndEvent_Error" />
  </bpmn:process>
